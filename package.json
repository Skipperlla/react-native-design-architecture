{
  "name": "stickersmash",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "check:lint": "eslint \"**/*.{js,ts,tsx}\"",
    "check:types": "tsc",
    "check": "npm run check:lint && npm run check:types",
    "lint": "eslint --fix",
    "check:prettier": "prettier --write",
    "build:dev:ios": "eas build --platform ios --profile development --non-interactive --local",
    "build:prod:ios": "eas build --platform ios --profile production --non-interactive --local",
    "build:dev:android": "eas build -p android --profile development --non-interactive --local",
    "build:prod:android": "eas build -p android --profile production --non-interactive --local",
    "build:dev:both": "yarn build:dev:ios && yarn build:dev:android",
    "build:prod:both": "yarn build:prod:ios && yarn build:prod:android",
    "clean:folders": "rm -rf android ios",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "test:maestro": "maestro test",
    "test:maestro:all": "maestro test .maestro"
  },
  "dependencies": {
    "@amplitude/analytics-react-native": "^1.4.9",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@gorhom/portal": "^1.0.14",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/analytics": "^20.4.0",
    "@react-native-firebase/app": "^20.4.0",
    "@react-native-firebase/crashlytics": "^20.4.0",
    "@react-native-firebase/remote-config": "^20.4.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^5.52.1",
    "axios": "^1.7.5",
    "expo": "^51.0.28",
    "expo-application": "~5.9.1",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.25",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-notifications": "~0.28.16",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.24",
    "i18n-js": "^4.4.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-icomoon": "^2.5.7",
    "react-native": "0.74.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "15.2.0",
    "semver": "^7.6.3",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@callstack/eslint-config": "^15.0.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@evilmartians/lefthook": "^1.7.14",
    "@react-native/eslint-config": "^0.75.2",
    "@react-native/typescript-config": "^0.75.2",
    "@types/react": "~18.2.45",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "commitlint": "^19.4.0",
    "eslint": "^8.19.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "react-native-dotenv": "^3.4.11",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "endOfLine": "auto",
    "useTabs": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "private": true
}
